{
  "name": "elysia-fullstack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "bun test",
    "test:unit": "bun test test/unit",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "start:server:dev": "bun --env-file=.env --watch src/app/server/index.ts",
    "build:server:server": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile app_server ./src/app/server/index.ts",
    "start:server:prod": "bun run build:server && ./app_server",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "db:migrate": "bunx prisma migrate dev --create-only --name init",
    "db:deploy": "bunx prisma migrate deploy",
    "db:generate": "bunx prisma generate",
    "db:dev:reset": "bunx prisma migrate reset --force",
    "typecheck": "bunx tsc --noEmit",
    "seed": "bun run src/scripts/seed.ts"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/opentelemetry": "^1.4.6",
    "@elysiajs/server-timing": "^1.4.0",
    "@elysiajs/static": "^1.4.6",
    "@hookform/resolvers": "^5.2.2",
    "@logtape/file": "^1.2.0",
    "@logtape/logtape": "^1.2.0",
    "@logtape/pretty": "^1.2.0",
    "@mantine/charts": "^8.3.8",
    "@mantine/core": "^8.3.8",
    "@mantine/dates": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@mantine/notifications": "^8.3.8",
    "@open-draft/deferred-promise": "^2.2.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@sapphire/snowflake": "^3.5.5",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "decimal.js": "^10.6.0",
    "elysia": "1.4.16",
    "elysia-xss": "^1.0.3",
    "elysiajs-helmet": "^1.0.2",
    "i18next": "^25.6.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "^6.1.2",
    "mantine-react-table-open": "^8.0.4",
    "numeral": "^2.0.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^16.3.3",
    "react-router": "^7.9.6",
    "rxjs": "^7.8.2",
    "superjson": "^2.2.5",
    "svg-captcha": "^1.4.0",
    "tailwindcss": "^4.1.17",
    "type-fest": "^5.2.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/bun": "^1.3.2",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "1.3.2",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3"
  },
  "module": "src/index.js"
}
