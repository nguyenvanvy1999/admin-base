{
  "name": "elysia-fullstack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "bun test",
    "test:unit": "bun test test/unit",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "start:server:dev": "bun --env-file=.env --watch src/app/server/index.ts",
    "build:server:server": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile app_server ./src/app/server/index.ts",
    "start:server:prod": "bun run build:server && ./app_server",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "db:migrate": "bunx prisma migrate dev --create-only --name init",
    "db:deploy": "bunx prisma migrate deploy",
    "db:generate": "bunx prisma generate",
    "db:dev:reset": "bunx prisma migrate reset --force",
    "typecheck": "bunx tsc --noEmit",
    "seed": "bun run src/scripts/seed.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@ant-design/pro-components": "^2.8.7",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/opentelemetry": "^1.4.8",
    "@elysiajs/server-timing": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "@logtape/file": "^1.2.0",
    "@logtape/logtape": "^1.2.0",
    "@logtape/pretty": "^1.2.0",
    "@open-draft/deferred-promise": "^2.2.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@sapphire/snowflake": "^3.5.5",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.90.11",
    "antd": "^5.23.1",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "decimal.js": "^10.6.0",
    "elysia": "1.4.16",
    "elysia-xss": "^1.0.3",
    "elysiajs-helmet": "^1.0.2",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jose": "^6.1.2",
    "numeral": "^2.0.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5",
    "react-router": "^7.9.6",
    "react-router-dom": "^7.9.6",
    "rxjs": "^7.8.2",
    "superjson": "^2.2.5",
    "svg-captcha": "^1.4.0",
    "tailwindcss": "^4.1.17",
    "type-fest": "^5.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@types/bun": "^1.3.3",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "1.3.3",
    "prisma": "^7.0.1",
    "typescript": "^5.9.3"
  },
  "module": "src/index.js"
}
