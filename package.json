{
  "name": "elysia-fullstack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build --compile --target bun --outfile server src/index.ts",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "db:migrate": "bunx prisma migrate dev --create-only --name init",
    "db:deploy": "bunx prisma migrate deploy",
    "db:generate": "bunx prisma generate",
    "db:dev:reset": "bunx prisma migrate reset --force"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/static": "^1.4.6",
    "@elysiajs/swagger": "^1.2.0",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@tanstack/react-query": "^5.90.7",
    "clsx": "^2.1.1",
    "elysia": "1.4.15",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5",
    "tailwindcss": "^4.1.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "^1.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "1.3.1",
    "prisma": "^6.19.0"
  },
  "module": "src/index.js"
}
