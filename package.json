{
  "name": "elysia-fullstack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build --compile --target bun --outfile server src/index.ts",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "db:migrate": "bunx prisma migrate dev --create-only --name init",
    "db:deploy": "bunx prisma migrate deploy",
    "db:generate": "bunx prisma generate",
    "db:dev:reset": "bunx prisma migrate reset --force",
    "seed": "bun run src/scripts/seed.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/static": "^1.4.6",
    "@elysiajs/swagger": "^1.2.0",
    "@logtape/file": "^1.1.2",
    "@logtape/logtape": "^1.1.2",
    "@logtape/pretty": "^1.1.2",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.90.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "elysia": "1.4.15",
    "i18next": "^25.6.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.4",
    "react-router": "^7.9.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "uuidv7": "^1.0.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "^1.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "1.3.2",
    "prisma": "^6.19.0"
  },
  "module": "src/index.js"
}
