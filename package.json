{
  "name": "elysia-fullstack-template",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build --compile --target bun --outfile server src/index.ts",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint --write .",
    "check": "bunx @biomejs/biome check --write .",
    "db:migrate": "bunx prisma migrate dev --create-only --name init",
    "db:deploy": "bunx prisma migrate deploy",
    "db:generate": "bunx prisma generate",
    "db:dev:reset": "bunx prisma migrate reset --force",
    "seed": "bun run src/scripts/seed.ts"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.6",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@hookform/resolvers": "^5.2.2",
    "@logtape/file": "^1.2.0",
    "@logtape/logtape": "^1.2.0",
    "@logtape/pretty": "^1.2.0",
    "@mantine/core": "^8.3.7",
    "@mantine/dates": "^8.3.7",
    "@mantine/hooks": "^8.3.7",
    "@mantine/notifications": "^8.3.7",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-tree-view": "^8.17.0",
    "@open-draft/deferred-promise": "^2.2.0",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.8.4",
    "dayjs": "^1.11.19",
    "elysia": "1.4.15",
    "i18next": "^25.6.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "mantine-react-table-open": "^8.0.4",
    "numeral": "^2.0.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^16.3.1",
    "react-router": "^7.9.5",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.17",
    "type-fest": "^5.2.0",
    "uuidv7": "^1.0.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/bun": "^1.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "1.3.2",
    "prisma": "^6.19.0"
  },
  "module": "src/index.js"
}
